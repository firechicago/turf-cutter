<%= form_for @search do |f| %>
<%if @search.errors.any? %>
<ul>
  <% @search.errors.full_messages.each do |msg|%>
  <li><%= msg %></li>
  <% end %>
</ul>
<% end %>
<div>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
</div>

<div>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
</div>

<div>
  <%= f.label :address1 %>
  <%= f.text_field :address1 %>
</div>

<div>
  <%= f.label :address2 %>
  <%= f.text_field :address2 %>
</div>

<div>
  <%= f.label :city %>
  <%= f.text_field :city %>
</div>

<div>
  <%= f.label :state %>
  <%= f.text_field :state %>
</div>

<div>
  <%= f.label :zip %>
  <%= f.text_field :zip %>
</div>

<%= f.submit 'Search' %>
<% end %>
